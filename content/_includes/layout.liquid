---
layoutType: content
---
<!DOCTYPE html>
<html lang="en">
<head>

  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <link rel="preload" href="/s/fonts/PublicSans[wght].woff2" as="font" type="font/woff2" crossorigin="anonymous">
  <link rel="stylesheet" href="/s/styles/styles.min.css?v={{ cacheBusters['styles'] }}">

  <title>{{ title | strip_html }} | John Brooks</title>
  {% if desc -%}
    <meta name="description" content="{{ desc | strip_html | escape }}">
  {%- endif %}

  <meta property="og:title" content="{{ title | strip_html | escape }} | John Brooks">
  {% if desc -%}
    <meta property="og:description" content="{{ desc | strip_html | escape }}">
  {%- endif %}
  <meta property="og:type" content="website">
  <meta property="og:url" content="https://www.pupismyname.com{{ page.url }}">
  <meta property="og:image" content="https://www.pupismyname.com{{ page.url }}og.png">
  <meta property="og:image:width" content="1200">
  <meta property="og:image:height" content="630">

  <link href="https://mastodon.social/@pupismyname" rel="me">

  <link rel="alternate" type="application/atom+xml" href="/feed.xml">

  <link rel="icon" type="image/png" sizes="16x16" href="/s/favicon/favicon-16x16.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/s/favicon/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="180x180" href="/s/favicon/favicon-180x180.png">
  <link rel="icon" type="image/png" sizes="192x192" href="/s/favicon/favicon-192x192.png">
  <link rel="icon" type="image/png" sizes="512x512" href="/s/favicon/favicon-512x512.png">
  <link rel="apple-touch-icon-precomposed" sizes="192x192" href="/s/favicon/favicon-192x192.png">

  <meta name="generator" content="{{ eleventy.generator }}">

</head>
<body class="layout-type-{{ layoutType }}">

  <div class="container">
    {{ content }}
  </div>

  {% comment -%}
    <div class="overlay"></div>
  {%- endcomment %}
  {% comment -%}
    <script src="/s/js/scroll-to-top.js?v={{ cacheBusters['js/scroll-to-top'] }}"></script>
  {%- endcomment %}

  {% if env.env != "development" -%}
    <script src="https://tinylytics.app/embed/2U8EGaiHgXFPAczhM2vS.js" defer></script>
  {%- endif %}

</body>
</html>
